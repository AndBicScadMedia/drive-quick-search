{
  "manifest_version": 2,
  "name": "Google Drive Quick Search",
  "description" : "Lets you quickly search and get results using the address bar",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "minimum_chrome_version": "29",
  "version": "0.1",
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "omnibox": { "keyword" : "drive" },
  "permissions": [
    "alarms",
    "identity",
    "storage"
  ],
  "oauth2": {
    "client_id": "TODO.apps.googleusercontent.com",
    "scopes": [ "https://www.googleapis.com/auth/drive.readonly" ]
  }
}
